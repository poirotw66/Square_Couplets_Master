{
  "name": "@justin_666/square-couplets-master-skills",
  "version": "1.1.0",
  "description": "Claude Agent Skills for generating Chinese New Year Doufang (diamond-shaped couplet) artwork using Google Gemini AI",
  "type": "module",
  "main": "bin/doufang-skills.js",
  "bin": {
    "doufang-skills": "bin/doufang-skills.js",
    "doufang": "bin/doufang-init.js",
    "doufang-prompt": "bin/doufang-prompt.js",
    "doufang-image": "bin/doufang-image.js",
    "doufang-optimize": "bin/doufang-optimize.js"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:cli": "tsc --project tsconfig.cli.json && node fix-imports.js",
    "preview": "vite preview",
    "prepublishOnly": "npm run build:cli && node -e \"const fs = require('fs'); ['doufang-skills.js', 'doufang-init.js', 'doufang-prompt.js', 'doufang-image.js', 'doufang-optimize.js'].forEach(f => fs.chmodSync('bin/' + f, 0o755));\""
  },
  "keywords": [
    "claude",
    "agent",
    "skills",
    "chinese",
    "calligraphy",
    "gemini",
    "ai",
    "art",
    "doufang",
    "chinese-new-year",
    "couplet",
    "traditional",
    "artwork"
  ],
  "author": "Justin",
  "license": "CC-BY-NC-SA-4.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poirotw66/Square_Couplets_Master.git"
  },
  "bugs": {
    "url": "https://github.com/poirotw66/Square_Couplets_Master/issues"
  },
  "homepage": "https://github.com/poirotw66/Square_Couplets_Master#readme",
  "files": [
    "bin/",
    "skills/",
    "dist/",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "@google/genai": "^1.37.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tsx": "^4.21.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  },
  "overrides": {
    "axios": ">=1.11.0",
    "qs": ">=6.14.1",
    "body-parser": ">=2.2.0"
  }
